    "# ...existing code...\n",
    "\n",
    "import warnings\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "# wycisz wszystkie warningi Pythona (ostrzegam: maskuje też ważne komunikaty)\n",
    "warnings.filterwarnings(\"ignore\")\n",
    "\n",
    "# wycisz specyficzne pandas / chained-assignment\n",
    "warnings.simplefilter(\"ignore\", category=pd.errors.SettingWithCopyWarning)\n",
    "pd.options.mode.chained_assignment = None\n",
    "\n",
    "# wycisz runtime warnings z NumPy (np. dzielenie przez 0)\n",
    "np.seterr(all='ignore')\n",
    "\n",
    "# helpery\n",
    "def first_existing(col_candidates):\n",
    "    for c in col_candidates:\n",
    "        if c in df_pivot.columns:\n",
    "            return c\n",
    "    return None\n",
    "\n",
    "# funkcja bezpiecznego dzielenia - POZA PĘTLĄ\n",
    "def safe_div(num, den):\n",
    "    \"\"\"Zwraca 0, gdy mianownik to 0 lub NaN.\"\"\"\n",
    "    if num is None or den is None:\n",
    "        return None\n",
    "    num = num.astype(float)\n",
    "    den = den.astype(float)\n",
    "    out = pd.Series(0.0, index=num.index)\n",
    "    mask = den.notna() & (den != 0)\n",
    "    out.loc[mask] = num.loc[mask] / den.loc[mask]\n",
    "    return out\n",
    "\n",
    "def sum_if_exist(col_list):\n",
    "    \"\"\"Sum columns that exist in df_pivot\"\"\"\n",
    "    existing = [df_pivot[c] for c in col_list if c and c in df_pivot.columns]\n",
    "    if not existing:\n",
    "        return None\n",
    "    return sum(existing)\n",
    "\n",
    "# upewnij się, że rok jest posortowany (potrzebne dynamiki)\n",
    "if \"rok\" in df_pivot.columns:\n",
    "    df_pivot = df_pivot.sort_values(\"rok\").reset_index(drop=True)\n",
    "\n",
    "# zbiór prefixów PKD (pierwsza część przed '_')\n",
    "pkds = sorted({c.split(\"_\")[0] for c in df_pivot.columns if c != \"rok\"})\n",
    "\n",
    "# dla każdego pkd obliczamy wskaźniki jeśli mamy wymagane kolumny\n",
    "for pk in pkds:\n",
    "    # możliwe nazwy bazowe\n",
    "    EN_col = first_existing([f\"{pk}_EN\", f\"{pk}_EN_upadlosci_y\", f\"{pk}_EN_upadlosci\"])\n",
    "    PEN_col = first_existing([f\"{pk}_PEN\"])\n",
    "    GS_col = first_existing([f\"{pk}_GS\"])\n",
    "    NP_col = first_existing([f\"{pk}_NP\"])\n",
    "    CF_col = first_existing([f\"{pk}_CF\"])\n",
    "    IO_col = first_existing([f\"{pk}_IO\"])\n",
    "    LTL_col = first_existing([f\"{pk}_LTL\"])\n",
    "    STL_col = first_existing([f\"{pk}_STL\"])\n",
    "    LTC_col = first_existing([f\"{pk}_LTC\"])\n",
    "    STC_col = first_existing([f\"{pk}_STC\"])\n",
    "    GS_I_col = first_existing([f\"{pk}_GS_I\"])\n",
    "    Przych_col = first_existing([f\"{pk}_Przych\", f\"{pk}_FIN\", f\"{pk}_FINANS\"])  # kilka możliwości\n",
    "    OP_col = first_existing([f\"{pk}_OP\"])\n",
    "    POS_col = first_existing([f\"{pk}_POS\"])\n",
    "    DEPR_col = first_existing([f\"{pk}_DEPR\"])\n",
    "    TC_col = first_existing([f\"{pk}_TC\"])\n",
    "    PPO_col = first_existing([f\"{pk}_PPO\"])\n",
    "    OFE_col = first_existing([f\"{pk}_OFE\"])\n",
    "    IP_col = first_existing([f\"{pk}_IP\"])\n",
    "    NWC_col = first_existing([f\"{pk}_NWC\"])\n",
    "    C_col = first_existing([f\"{pk}_C\"])\n",
    "    INV_col = first_existing([f\"{pk}_INV\"])\n",
    "    REC_col = first_existing([f\"{pk}_REC\"])\n",
    "\n",
    "    # convenience sums\n",
    "    DEBT_series = sum_if_exist([LTL_col, STL_col]) if (LTL_col or STL_col) else None\n",
    "    BANK_LOANS_series = sum_if_exist([LTC_col, STC_col]) if (LTC_col or STC_col) else None\n",
    "    LTLplusSTL_col_exists = DEBT_series is not None\n",
    "    LTCplusSTC_col_exists = BANK_LOANS_series is not None\n",
    "\n",
    "    # 1 Udział firm rentownych PEN / EN\n",
    "    if PEN_col and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Udział firm rentownych\"] = safe_div(df_pivot[PEN_col], df_pivot[EN_col])\n",
    "\n",
    "    # 2 Średnie przychody na firmę GS / EN\n",
    "    if GS_col and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średnie przychody na firmę\"] = safe_div(df_pivot[GS_col], df_pivot[EN_col])\n",
    "\n",
    "    # 3 Średni zysk netto na firmę NP / EN\n",
    "    if NP_col and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średni zysk netto na firmę\"] = safe_div(df_pivot[NP_col], df_pivot[EN_col])\n",
    "\n",
    "    # 4 Średni cash flow na firmę CF / EN\n",
    "    if CF_col and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średni CF na firmę\"] = safe_div(df_pivot[CF_col], df_pivot[EN_col])\n",
    "\n",
    "    # 5 Średnie nakłady inwestycyjne na firmę IO / EN\n",
    "    if IO_col and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średnie nakłady inwestycyjne na firmę\"] = safe_div(df_pivot[IO_col], df_pivot[EN_col])\n",
    "\n",
    "    # 6 Średni poziom długu na firmę (LTL + STL) / EN\n",
    "    if LTLplusSTL_col_exists and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średni poziom długu na firmę\"] = safe_div(DEBT_series, df_pivot[EN_col])\n",
    "\n",
    "    # 7 Średni poziom kredytów bankowych na firmę (LTC + STC) / EN\n",
    "    if LTCplusSTC_col_exists and EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Średni poziom kredytów bankowych na firmę\"] = safe_div(BANK_LOANS_series, df_pivot[EN_col])\n",
    "\n",
    "    # 8 Udział sprzedaży netto w przychodach ogółem GS_I / GS\n",
    "    if GS_I_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Udział sprzedaży netto\"] = safe_div(df_pivot[GS_I_col], df_pivot[GS_col])\n",
    "\n",
    "    # 9 Udział przychodów finansowych w przychodach ogółem Przych / GS\n",
    "    if Przych_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Udział przychodów finansowych\"] = safe_div(df_pivot[Przych_col], df_pivot[GS_col])\n",
    "\n",
    "    # 10 Marża netto NP / GS\n",
    "    if NP_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Marża netto (Net Margin)\"] = safe_div(df_pivot[NP_col], df_pivot[GS_col])\n",
    "\n",
    "    # 11 Marża operacyjna OP / GS\n",
    "    if OP_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Marża operacyjna (Operating Margin)\"] = safe_div(df_pivot[OP_col], df_pivot[GS_col])\n",
    "\n",
    "    # 12 Marża na sprzedaży POS / GS_I\n",
    "    if POS_col and GS_I_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Marża na sprzedaży\"] = safe_div(df_pivot[POS_col], df_pivot[GS_I_col])\n",
    "\n",
    "    # 13 Marża CF CF / GS\n",
    "    if CF_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Marża CF (CF Margin)\"] = safe_div(df_pivot[CF_col], df_pivot[GS_col])\n",
    "\n",
    "    # 14 Marża EBITDA proxy (OP + DEPR) / GS_I\n",
    "    if OP_col and DEPR_col and GS_I_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Marża EBITDA (proxy)\"] = safe_div(df_pivot[OP_col].add(df_pivot[DEPR_col], fill_value=0), df_pivot[GS_I_col])\n",
    "\n",
    "    # 15 Udział kosztów w przychodach TC / GS\n",
    "    if TC_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Udział kosztów w przychodach\"] = safe_div(df_pivot[TC_col], df_pivot[GS_col])\n",
    "\n",
    "    # 16 PPO / GS\n",
    "    if PPO_col and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Udział PPO w przychodach\"] = safe_div(df_pivot[PPO_col], df_pivot[GS_col])\n",
    "\n",
    "    # 17 Udział działalności finansowej w wyniku netto (Przych - OFE - IP) / NP\n",
    "    if Przych_col and NP_col:\n",
    "        numer = df_pivot[Przych_col]\n",
    "        if OFE_col:\n",
    "            numer = numer.sub(df_pivot[OFE_col], fill_value=0)\n",
    "        if IP_col:\n",
    "            numer = numer.sub(df_pivot[IP_col], fill_value=0)\n",
    "        df_pivot.loc[:, f\"{pk}_Udział działalności fin. w NP\"] = safe_div(numer, df_pivot[NP_col])\n",
    "\n",
    "    # 18 CF / NP\n",
    "    if CF_col and NP_col:\n",
    "        df_pivot.loc[:, f\"{pk}_Relacja CF do NP\"] = safe_div(df_pivot[CF_col], df_pivot[NP_col])\n",
    "\n",
    "    \n",
    "    # 20 Dynamiki (pct_change) dla kilku kluczowych wskaźników — dodajemy kolumny z sufiksem _dyn\n",
    "    # zapewniamy, że operacja jest możliwa tylko gdy kolumna istnieje\n",
    "    if GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_GS_dyn\"] = df_pivot[GS_col].pct_change()\n",
    "    if GS_I_col:\n",
    "        df_pivot.loc[:, f\"{pk}_GS_I_dyn\"] = df_pivot[GS_I_col].pct_change()\n",
    "    if NP_col:\n",
    "        df_pivot.loc[:, f\"{pk}_NP_dyn\"] = df_pivot[NP_col].pct_change()\n",
    "    if CF_col:\n",
    "        df_pivot.loc[:, f\"{pk}_CF_dyn\"] = df_pivot[CF_col].pct_change()\n",
    "    if EN_col:\n",
    "        df_pivot.loc[:, f\"{pk}_EN_dyn\"] = df_pivot[EN_col].pct_change()\n",
    "    if IO_col:\n",
    "        df_pivot.loc[:, f\"{pk}_IO_dyn\"] = df_pivot[IO_col].pct_change()\n",
    "    if LTL_col or STL_col:\n",
    "        # dynamika zadłużenia ogółem\n",
    "        if DEBT_series is not None:\n",
    "            df_pivot.loc[:, f\"{pk}_DEBT_dyn\"] = DEBT_series.pct_change()\n",
    "    if LTC_col or STC_col:\n",
    "        if BANK_LOANS_series is not None:\n",
    "            df_pivot.loc[:, f\"{pk}_BANK_LOANS_dyn\"] = BANK_LOANS_series.pct_change()\n",
    "\n",
    "    # 21 Zmiana udziału firm rentownych (PEN/EN)_t - (PEN/EN)_{t-1}\n",
    "    if PEN_col and EN_col:\n",
    "        share = safe_div(df_pivot[PEN_col], df_pivot[EN_col])\n",
    "        if share is not None:\n",
    "            df_pivot.loc[:, f\"{pk}_Zmiana udziału firm rentownych\"] = share - share.shift(1)\n",
    "\n",
    "    # 22 DEBT / GS, LTL/GS, STL/GS\n",
    "    if DEBT_series is not None and GS_col:\n",
    "        df_pivot.loc[:, f\"{pk}_DEBT_to_GS\"] = safe_div(DEBT_series, df_pivot[GS_col])\n",
